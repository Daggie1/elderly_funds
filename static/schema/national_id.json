{
  "title": "Birth Certificate",
  "type": "object",
  "required": [
    "full_names",
    "date_of_birth",
    "district",
    "gender",
    "division",
    "location",
    "sub-location"
  ],
  "properties": {
    "full_names": {
      "type": "string",
      "description": "First and Last name",
      "minLength": 4,
      "default": "Enter your Name"
    },

    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "date_of_birth": {
      "type": "string",
      "format": "date",
      "options": {
        "flatpickr": {}
      }
    },
    "district": {
      "type": "string",
      "title": "district",
      "description": "Place of Birth",
      "default": "Kenya"
    },
 "division": {
      "type": "string",
      "title": "division",
      "description": "Place of Birth",
      "default": "Kenya"
    },
 "location": {
      "type": "string",
      "title": "Location",
      "description": "Place of Birth",
      "default": "Kenya"
    },
 "sub-location": {
      "type": "string",
      "title": "sub-location",
      "description": "Place of Birth",
      "default": "Kenya"
    }
  }
}